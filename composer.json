{
    "name": "lukasbableck/contao-tabs-bundle",
    "type": "contao-bundle",
	"description": "This bundle provides a tabs element for Contao. It allows you to create tabbed content sections easily.",
	"keywords": [
		"contao",
		"tabs"
	],
    "license": "LGPL-3.0-or-later",
	"authors": [
		{
			"name": "Lukas Bableck",
			"email": "lukas@bableck.dev",
			"homepage": "https://bableck.dev"
		}
	],
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^5.3"
    },
	"conflict": {
		"contao/manager-plugin": "<2.0 || >=3.0"
	},
	"require-dev": {
		"contao/manager-plugin": "^2.0",
		"friendsofphp/php-cs-fixer": "^3.57",
        "vincentlanglet/twig-cs-fixer": "^3.5"
	},
    "autoload": {
        "psr-4": {
            "Lukasbableck\\ContaoTabsBundle\\": "src/"
        }
    },
	"extra": {
		"contao-manager-plugin": "Lukasbableck\\ContaoTabsBundle\\ContaoManager\\Plugin"
	}
}
